# Simulating financial transaction data
financial_data = {
    'Transaction_ID': [1, 2, 3, 4, 5, 6],
    'Date': ['2024-11-01', '2024-11-02', '2024-11-03', '2024-11-04', '2024-11-05', '2024-11-06'],
    'Revenue': [1000, 1200, 900, 1500, 1100, 1300],
    'Expenses': [400, 500, 350, 600, 450, 500]
}

financial_df = pd.DataFrame(financial_data)
financial_df['Profit'] = financial_df['Revenue'] - financial_df['Expenses']

print(financial_df)
